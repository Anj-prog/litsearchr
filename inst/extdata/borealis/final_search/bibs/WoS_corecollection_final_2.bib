Editor = {{Eversole, AG}},
Title = {{The Texas Parks and Wildlife Department Central Coast Wetland Ecosystems
   Project: A new approach at integrating nongame interests with existing
   game management programs}},
Booktitle = {{PROCEEDINGS OF THE FORTY-NINTH ANNUAL CONFERENCE - SOUTHEASTERN
