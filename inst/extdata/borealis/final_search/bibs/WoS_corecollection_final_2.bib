@article{ ISI:000081562400002,
Author = {Haney, JC and Lydic, J},
Title = {{Avifauna and vegetation structure in an old-growth oak-pine forest on
   the Cumberland Plateau, Tennessee (USA)}},
Journal = {{NATURAL AREAS JOURNAL}},
Year = {{1999}},
Volume = {{19}},
Number = {{3}},
Pages = {{199-210}},
Month = {{JUL}},
Abstract = {{We tested whether the breeding bird community and vegetation structure
   in an old-growth mixed pine-hardwood forest in the Savage Gulf State
   Natural Area, Cumberland Plateau, Tennessee differed from those found in
   younger forest. Compared to breeding bird censuses conducted in
   secondary stands of the same forest type, total breeding-season density
   of birds was 63\% higher in the Savage Gulf old growth. Large, widely
   distributed softwood snags enhanced the abundances of both primary and
   secondary cavity-nesting birds. Like other eastern old-growth forests
   with complex canopies, the Savage Gulf oak-pine forest contained high
   densities of arboreal Parulinae: yellow-throated warbler (Dendroica
   dominica {[}L.]) and pine warbler (D. pinus {[}Wilson]). Two raptors
   more commonly associated with riparian and mesic forests, the
   red-shouldered hawk (Buteo lineatus {[}Gmelin]) and barred owl (Strix
   varia Barton), were unexpected breeding residents in this well-drained,
   upland forest site. Two species of pines-shortleaf (Pinus echinata
   Miller) and Virginia (P. virginiana Miller)-comprised 29\% of the total
   basal area and 40\% of stem density of trees greater than or equal to
   7.6 cm dbh. Pines formed a dispersed emergent canopy (to 39 m) over a
   lower canopy dominated by white oak (Quercus alba L.) and northern red
   oak (Q. rubra L.). Individual shortleaf pines exceeded 84 cm dbh and
   were 230-270 years of age. Measurements of eight attributes of
   vegetation structure generally met or exceeded values proposed
   independently for old-growth condition in this forest type.}},
