@article{ ISI:A1993LA01200018,
Author = {BULL, EL and HOLTHAUSEN, RS},
Title = {{HABITAT USE AND MANAGEMENT OF PILEATED WOODPECKERS IN NORTHEASTERN
   OREGON}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
Year = {{1993}},
Volume = {{57}},
Number = {{2}},
Pages = {{335-345}},
Month = {{APR}},
Abstract = {{We determined home range size and habitats used by pileated woodpeckers
   (Dryocopus pileatus) to provide essential information for proper
   management of the species in northeastern Oregon. Twenty-three pileated
   woodpeckers fitted with transmitters were followed for 5-10 months
   (Jun-Mar) during 1989-90. Mated pairs (n = 7) ranged over smaller areas
   (xBAR = 407 ha) than birds (xBAR = 597 ha) whose mates had died (n = 9).
   Habitat use within home ranges was not random. Stands with old growth,
   grand fir (Abies grandis), no logging, and greater-than-or-equal-to 60\%
   canopy closure were used more (P < 0.01) than expected, and all other
   types of stands were used less than expected. From June until March,
   38\% of the observations of foraging were on downed logs, 38\% on dead
   trees, 18\% on live trees, and 6\% on stumps. We recommend that
   management for pileated woodpeckers in northeastern Oregon include
   increasing density of snags for nesting and foraging, increasing density
   of downed logs in foraging areas, and increasing management areas from
   the existing 121 ha to 364 ha of forest. Within these areas, we
   recommend that 75\% be in grand fir forest type; 25\% be old growth; and
   the remainder be mature stands; at least 50\% have
   greater-than-or-equal-to 60\% canopy closure; and at least 40\% be
   unlogged with the remainder in mature stands.}},
