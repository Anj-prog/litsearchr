@article{ ISI:A1992HJ93100007,
Author = {ZIMMERMAN, JL},
Title = {{DENSITY-INDEPENDENT FACTORS AFFECTING THE AVIAN DIVERSITY OF THE
   TALLGRASS PRAIRIE COMMUNITY}},
Journal = {{WILSON BULLETIN}},
Year = {{1992}},
Volume = {{104}},
Number = {{1}},
Pages = {{85-94}},
Month = {{MAR}},
Abstract = {{The low species richness in grassland bird communities has been
   hypothesized to result from competition in a simplistic, saturated
   habitat (Cody) or to result from the periodic occurrence of climatic
   extremes that prevent drought-susceptible species from perennial
   settlement (Wiens). Predictions based on these two hypotheses were
   evaluated using June population estimates on burned and unburned
   tallgrass prairie over the decade, 1981-1990, on the Konza Prairie
   Research Natural Area, Kansas. The results of these analyses support the
   notion that drought tolerance is the primary factor that determines
   membership in the tallgrass prairie community. When drought and fire are
   coincidental, however, the community does respond in a manner
   commensurate with the saturation hypothesis by a reduction in bird
   abundances but not a decrease in species richness.}},
