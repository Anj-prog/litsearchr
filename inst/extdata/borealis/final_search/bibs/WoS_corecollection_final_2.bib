@article{ ISI:A1994NY50700011,
Author = {COWLING, RM and MUSTART, PJ and LAURIE, H and RICHARDS, MB},
Title = {{SPECIES-DIVERSITY - FUNCTIONAL DIVERSITY AND FUNCTIONAL REDUNDANCY IN
   FYNBOS COMMUNITIES}},
Journal = {{SOUTH AFRICAN JOURNAL OF SCIENCE}},
Year = {{1994}},
Volume = {{90}},
Number = {{6}},
Pages = {{333-337}},
Month = {{JUN}},
Note = {{Symposium on Biotic Diversity and the Function of Mediterranean-type
   Ecosystems, NATL BOTAN INST, CLAREMONT, SOUTH AFRICA, SEP 07-11, 1992}},
Abstract = {{Cape fynbos communities are species-rich but apparently include many
   species with similar growth forms. Many of these species may be
   functional analogues with similar pathways for nutrient and water
   cycling, and carbon flux. This apparent functional redundancy suggests
   that the extinction of some analogues should have little impact on
   ecosystem-level processes. Much of the biological diversity in fynbos is
   associated with postfire regeneration traits which facilitate
   competition for space. Lottery and regeneration niche models can explain
   the coexistence of functionally analagous proteoid and ericoid shrubs.
   The diversity of regeneration traits within functional types provides a
   `backup' in the event of fire-related stochastic population
   fluctuations. This ensures long-term stability of community structure,
   possibly maintaining functional diversity and ecosystem processes.
   Research to test hypotheses on the relationships between species
   diversify and ecosystem processes in fynbos communities may lack a
   tractable protocol; some directions are, however, given.}},
