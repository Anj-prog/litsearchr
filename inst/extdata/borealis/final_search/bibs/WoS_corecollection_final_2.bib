@inproceedings{ ISI:A1994BJ69M00046,
Author = {Krusac, DL and Dabney, JM and Petrick, JJ},
Editor = {{Eversole, AG}},
Title = {{An ecological approach to managing southern national forests for
   red-cockaded woodpecker recovery}},
Booktitle = {{PROCEEDINGS OF THE FORTY-EIGHTH ANNUAL CONFERENCE - SOUTHEASTERN
   ASSOCIATION OF FISH AND WILDLIFE AGENCIES}},
Year = {{1994}},
Pages = {{374-382}},
Note = {{48th Annual Conference of
   Southeastern-Association-of-Fish-and-Wildlife-Agencies, BILOXI, MS, OCT
   23-26, 1994}},
Organization = {{SE Assoc Fish \& Wildlife Agencies}},
Abstract = {{The U.S. Department of agriculture, Forest Service red-cockaded
   woodpecker (Picoides borealis) recovery strategy is based on
   conservation biology principles. It implements landscape-scale
   management by identifying 26 habitat management areas (HMAs) totaling
   nearly 810,000 hectares. Within these designated HMAs, longer timber
   harvesting rotations will be established. Management intensity levels
   (MILs) will be established based on red-cockaded woodpecker (RCW)
   population size. The smallest populations will receive the most
   intensive direct RCW management while being most restrictive regarding
   forest management. The combination of identifying HMAs and implementing
   longer timber harvesting rotations and appropriate MILs should overcome
   the effects of past fragmentation and demographic isolation. Prescribed
   burning regimes will mimic historical fire regimes of southern pine
   ecosystems.}},
