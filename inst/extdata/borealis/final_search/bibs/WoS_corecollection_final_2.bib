@inproceedings{ ISI:000171432000018,
Author = {Rumble, MA and Flake, LD and Mills, TR and Dykstra, BL},
Book-Group-Author = {{USDA
   USDA}},
Title = {{Do pine trees in aspen stands increase bird diversity?}},
Booktitle = {{SUSTAINING ASPEN IN WESTERN LANDSCAPES: SYMPOSIUM PROCEEDINGS}},
Series = {{USDA FOREST SERVICE GENERAL TECHNICAL REPORT ROCKY MOUNTAIN}},
Year = {{2001}},
