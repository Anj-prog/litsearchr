@ARTICLE{Rolo2017788,
author={Rolo, V. and Olivier, P.I. and van Aarde, R.},
title={Tree and bird functional groups as indicators of recovery of regenerating subtropical coastal dune forests},
journal={Restoration Ecology},
year={2017},
