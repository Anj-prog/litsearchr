@ARTICLE{Lewis201482,
author={Lewis, R.J. and Pakeman, R.J. and Angus, S. and Marrs, R.H.},
title={Using compositional and functional indicators for biodiversity conservation monitoring of semi-natural grasslands in Scotland},
journal={Biological Conservation},
year={2014},
volume={175},
