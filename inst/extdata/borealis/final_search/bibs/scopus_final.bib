@ARTICLE{Meyer200171,
author={Meyer, D.L. and Johnson, J.M. and Gill, J.W.},
title={Comparison of nekton use of Phragmites australis and Spartina alterniflora marshes in the chesapeake bay, USA},
journal={Marine Ecology Progress Series},
