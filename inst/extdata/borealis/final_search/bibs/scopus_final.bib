@ARTICLE{Buenau2014964,
author={Buenau, K.E. and Hiller, T.L. and Tyre, A.J.},
title={Modelling the effects of river flow on population dynamics of piping plovers (Charadrius melodus) and least terns (Sternula antillarum) nesting on the Missouri river},
