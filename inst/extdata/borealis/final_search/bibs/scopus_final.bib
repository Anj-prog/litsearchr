@ARTICLE{Fogg2014,
author={Fogg, A.M. and Jay Roberts, L. and Burnett, R.D.},
title={Occurrence patterns of black-backed woodpeckers in green forest of the Sierra Nevada Mountains, California, USA [Tendance dans l’occurrence du pic à dos noir dans les forêts vertes de la sierra nevada, Californie, États-unis]},
journal={Avian Conservation and Ecology},
year={2014},
