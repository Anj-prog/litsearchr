@ARTICLE{Garcia-Del-Rey2010298,
author={Garcia-Del-Rey, E. and Otto, R. and Fernández-Palacios, J.M. and Muñoz, P.G. and Gil, L.},
title={Effects of wildfire on endemic breeding birds in a Pinus canariensis Forest of Tenerife, Canary Islands},
