@ARTICLE{Badenhausser20121050,
author={Badenhausser, I. and Gouat, M. and Goarant, A. and Cornulier, T. and Bretagnolle, V.},
title={Spatial autocorrelation in farmland grasshopper assemblages (orthoptera: Acrididae) in Western France},
journal={Environmental Entomology},
