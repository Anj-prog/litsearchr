@ARTICLE{Tharme2001439,
author={Tharme, A.P. and Green, R.E. and Baines, D. and Bainbridge, I.P. and O'Brien, M.},
title={The effect of management for red grouse shooting on the population density of breeding birds on heather-dominated moorland},
journal={Journal of Applied Ecology},
