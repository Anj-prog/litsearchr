@ARTICLE{White2013261,
author={White, A.M. and Zipkin, E.F. and Manley, P.N. and Schlesinger, M.D.},
title={Simulating avian species and foraging group responses to fuel reduction treatments in coniferous forests},
journal={Forest Ecology and Management},
