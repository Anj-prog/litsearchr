}

@ARTICLE{Ponce20142834,
author={Ponce, C. and Bravo, C. and Alonso, J.C.},
title={Effects of agri-environmental schemes on farmland birds: Do food availability measurements improve patterns obtained from simple habitat models?},
journal={Ecology and Evolution},
year={2014},
