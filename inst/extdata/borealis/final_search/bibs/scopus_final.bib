@ARTICLE{Davidsdottir201621,
author={Davidsdottir, B. and Gunnarsson, T.G. and Halldorsson, G. and Sigurdsson, B.D.},
title={Avian abundance and communities in areas revegetated with exotic versus native plant species},
journal={Icelandic Agricultural Sciences},
year={2016},
