@ARTICLE{Smith2016312,
author={Smith, P. and Smith, J.},
title={Influence of fire regime and other habitat factors on a eucalypt forest bird community in south-eastern Australia in the 1980s},
journal={Australian Journal of Zoology},
year={2016},
volume={64},
