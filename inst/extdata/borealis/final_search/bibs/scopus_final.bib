@ARTICLE{Volk200674,
author={Volk, M. and Bungener, P. and Contat, F. and Montani, M. and Fuhrer, J.},
title={Grassland yield declined by a quarter in 5 years of free-air ozone fumigation},
journal={Global Change Biology},
year={2006},
volume={12},
