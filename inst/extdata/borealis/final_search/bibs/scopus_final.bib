@ARTICLE{Burgess2016687,
author={Burgess, E.E. and Maron, M.},
title={Does the response of bird assemblages to fire mosaic properties vary among spatial scales and foraging guilds?},
journal={Landscape Ecology},
year={2016},
volume={31},
