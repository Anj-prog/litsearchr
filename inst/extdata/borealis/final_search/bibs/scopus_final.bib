@ARTICLE{Bell2015,
author={Bell, D. and Hjältén, J. and Nilsson, C. and JØrgensen, D. and Johansson, T.},
title={Forest restoration to attract a putative umbrella species, the white-backed woodpecker, benefited saproxylic beetles},
