@ARTICLE{Bennett20111,
author={Bennett, V.J. and Smith, W.P. and Betts, M.G.},
title={Toward understanding the ecological impact of transportation corridors},
journal={USDA Forest Service - General Technical Report PNW-GTR},
year={2011},
number={846},
pages={1-40},
note={cited By 1},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-79961205330&partnerID=40&md5=31edb6e5843b841207c2181fc664e83e},
affiliation={Department of Forest Ecosystems and Society, Oregon State University, Richardson Hall, Corvallis, OR 97331, United States; U.S. Department of Agriculture, Forest Service, Pacific Northwest Research Station, Forestry Sciences Laboratory, 3625 93rd Avenue SW, Olympia, WA 98512-9193, United States},
abstract={Transportation corridors (notably roads) affect wildlife habitat, populations, and entire ecosystems. Considerable effort has been expended to quantify direct effects of roads on wildlife populations and ecological communities and processes. Much less effort has been expended toward quantifying indirect effects. In this report, we provide a comprehensive review of road/transportation corridor ecology; in particular, how this new field of ecology has advanced worldwide. Further, we discuss how research thus far has shaped our understanding and views of the ecological implications of transportation infrastructures, and, in turn, how this has led to the current guidance, policies, and management options. We learned that the impacts of transportation infrastructures are a global issue, with the potential to affect a wide variety of taxonomically diverse species and ecosystems. Because the majority of research to date has focused on the direct and more aesthetic and anthropocentric implications of transportation corridors, mainly wildlife-vehicle collisions, it is a fairly standard practice to incorporate underpasses, green bridges (i.e., overpasses), fencing, and barriers into road corridors to alleviate such impacts. Few studies, however, have been able to demonstrate the efficiency of these structures. Furthermore, it is becoming increasingly evident that the indirect implications of transportation infrastructures (i.e., behavioral responses of wildlife individuals to roads) may be more pervasive, at least from the standpoint of biological diversity. Understanding how road corridors influence the functional connectivity of landscapes is crucial if we are to effectively manage species of concern. With these issues in mind, we propose a program of study that addresses the indirect and cumulative implications of transportation infrastructure on species distributions, community structure and ecosystem function.},
correspondence_address1={Bennett, V. J.; Department of Forest Ecosystems and Society, Oregon State University, Richardson Hall, Corvallis, OR 97331, United States},
issn={08874840},
language={English},
document_type={Article},
source={Scopus},
}

@ARTICLE{Rahman20111879,
author={Rahman, M.L. and Tarrant, S. and McCollin, D. and Ollerton, J.},
title={The conservation value of restored landfill sites in the East Midlands, UK for supporting bird communities},
journal={Biodiversity and Conservation},
year={2011},
volume={20},
