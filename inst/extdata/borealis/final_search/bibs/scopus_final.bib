@ARTICLE{Law201458,
author={Law, B.S. and Chidel, M. and Brassil, T. and Turner, G. and Kathuria, A.},
title={Trends in bird diversity over 12years in response to large-scale eucalypt plantation establishment: Implications for extensive carbon plantings},
