@ARTICLE{Lamb2019,
author={Lamb, A. and Finch, T. and Pearce-Higgins, J.W. and Ausden, M. and Balmford, A. and Feniuk, C. and Hirons, G. and Massimino, D. and Green, R.E.},
title={The consequences of land sparing for birds in the United Kingdom},
journal={Journal of Applied Ecology},
