@ARTICLE{Ashbrook2016583,
author={Ashbrook, K. and Taylor, A. and Jane, L. and Carter, I. and Székely, T.},
title={Impacts of survival and reproductive success on the long-term population viability of reintroduced great bustards Otis tarda in the UK},
journal={ORYX},
