@ARTICLE{Darmon2014319,
author={Darmon, G. and Hidding, B. and De Bellefeuille, S. and Tremblay, J.-P. and Côté, S.D.},
title={A generalist rodent benefits from logging regardless of deer density},
journal={Ecoscience},
