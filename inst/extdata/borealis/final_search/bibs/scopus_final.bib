@ARTICLE{Kirin2011467,
author={Kirin, T. and Kralj, J. and Čikovič, D. and Dolenec, Z.},
title={Habitat selection and similarity of the forest songbird communities in medvednica and žumberak - Samoborsko gorje nature parks},
journal={Sumarski List},
year={2011},
volume={135},
