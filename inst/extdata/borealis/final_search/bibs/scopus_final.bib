}

@ARTICLE{Schlacher201728,
author={Schlacher, T.A. and Hutton, B.M. and Gilby, B.L. and Porch, N. and Maguire, G.S. and Maslo, B. and Connolly, R.M. and Olds, A.D. and Weston, M.A.},
title={Algal subsidies enhance invertebrate prey for threatened shorebirds: A novel conservation tool on ocean beaches?},
