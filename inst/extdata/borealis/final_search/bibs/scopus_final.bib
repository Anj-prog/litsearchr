@ARTICLE{Kurek20141,
author={Kurek, P. and Kapusta, P. and Holeksa, J.},
title={Burrowing by badgers (Meles meles) and foxes (Vulpes vulpes) changes soil conditions and vegetation in a European temperate forest},
journal={Ecological Research},
