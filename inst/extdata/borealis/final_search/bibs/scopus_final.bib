@ARTICLE{Dolman2007146,
author={Dolman, P.M. and Hinsley, S.A. and Bellamy, P.E. and Watts, K.K.},
title={Woodland birds in patchy landscapes: The evidence base for strategic networks},
journal={Ibis},
