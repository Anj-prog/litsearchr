@ARTICLE{Hartmann2015154,
author={Hartmann, S.A. and Segelbacher, G. and Juiña, M.E. and Schaefer, H.M.},
title={Effects of habitat management can vary over time during the recovery of an endangered bird species},
journal={Biological Conservation},
