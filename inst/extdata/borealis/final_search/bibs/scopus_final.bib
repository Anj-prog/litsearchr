@ARTICLE{Muller-Karger201752,
author={Muller-Karger, F.E. and Rueda-Roa, D. and Chavez, F.P. and Kavanaugh, M.T. and Roffer, M.A.},
title={Megaregions among the large marine ecosystems of the Americas},
