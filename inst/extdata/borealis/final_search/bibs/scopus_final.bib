@ARTICLE{Askins2012,
author={Askins, R.A. and Folsom-O'Keefe, C.M. and Hardy, M.C.},
title={Effects of vegetation, corridor width and regional land use on early successional birds on powerline corridors},
journal={PLoS ONE},
year={2012},
