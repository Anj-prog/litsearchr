@ARTICLE{Cruz-Angón2005150,
author={Cruz-Angón, A. and Greenberg, R.},
title={Are epiphytes important for birds in coffee plantations? An experimental assessment},
journal={Journal of Applied Ecology},
year={2005},
volume={42},
