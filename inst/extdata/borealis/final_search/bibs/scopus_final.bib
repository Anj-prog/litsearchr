@ARTICLE{Stirnemann2015733,
author={Stirnemann, I.A. and Ikin, K. and Gibbons, P. and Blanchard, W. and Lindenmayer, D.B.},
title={Measuring habitat heterogeneity reveals new insights into bird community composition},
